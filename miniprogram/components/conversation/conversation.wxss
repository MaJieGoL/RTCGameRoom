.chat {
  width: 100%;
  position: fixed;
  bottom: 45px;
  top: 95px;
  display: block;
  background-color: #EFEFF4;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}

.goodPeople {
  color: #0079FF !important;
}

.self-chat {
  display: inline-block;
  position: relative;
  float: right;
}

.self-chat view:after {
  position: absolute;
  top: 14px;
  border: 6px solid transparent;
  content: " ";
  border-left-color: #b2e281;
  border-left-width: 4px;
  right: 0;
}

.self-chat view text {
  padding: 9px 12px;
  border-radius: 3px;
  word-wrap: break-word;
  word-break: break-all;
  min-height: 25px;
  font-size: 14px;
  margin: 0 10px;
  vertical-align: top;
  display: block;
  font-family: inherit;
  white-space: pre-wrap;
  word-break: normal;
  display: block;
  max-width: 500px;
  min-height: 1em;
  display: inline-block;
  vertical-align: top;
  position: relative;
  text-align: left;
  float: right;
  background-color: #b2e281;
  max-width: 280px;
}

.other-chat {
  display: inline-block;
  position: relative;
}

.other-chat view:before {
  position: absolute;
  top: 14px;
  border: 6px solid transparent;
  content: " ";
  border-left-width: 4px;
  border-right-color: #fff;
}

.chat-view-child-1 {
  font-size:9px;
  padding-left:11px;
}

.other-chat text {
  padding: 9px 12px;
  border-radius: 3px;
  word-wrap: break-word;
  word-break: break-all;
  min-height: 25px;
  font-size: 14px;
  margin: 0 10px;
  vertical-align: top;
  display: block;
  font-family: inherit;
  white-space: pre-wrap;
  word-break: normal;
  display: block;
  max-width: 500px;
  min-height: 1em;
  background-color: #fff;
  display: inline-block;
  vertical-align: top;
  position: relative;
  text-align: left;
  max-width: 280px;
}

.self-img .avatar{
